{
  "name": "block-proxy",
  "version": "0.1.0",
  "private": true,
  "description": "Small-scale network proxy filter",
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayli/block-proxy.git"
  },
  "scripts": {
    "dev": "cp ./hack-of-anyproxy/lib/requestHandler.js ./node_modules/anyproxy/lib/ && PORT=8003 craco start",
    "start": "npm run dev",
    "build": "react-scripts build",
    "docker:build": "docker build -t block-proxy .",
    "docker:build_arm": "docker buildx build --platform linux/arm64/v8 -t block-proxy .",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "anyproxy": "^4.1.3",
    "@craco/craco": "^7.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
